itialInCartProducts = products.products.reduce((acc, curr) => {\n                    return { ...acc, [curr.id]: 0 }\n                }, {})\n        \n                setAvailableProducts(initialAvailableProducts)\n                setInCartProducts(initialInCartProducts)\n            })\n    }, [])\n\n    // Add a product to cart. \n    // Updating corresponding quantities in availableProducts and inCartProducts \n    const onAddToCart = (productId) => {\n        const availableBeforeAdd = availableProducts[productId]\n        if (availableBeforeAdd>0){\n        setAvailableProducts({\n            ...availableProducts,\n            [productId]: availableBeforeAdd - 1,\n        })\n        const inCartBeforeAdd = inCartProducts[productId]\n        setInCartProducts({\n            ...inCartProducts,\n            [productId]: inCartBeforeAdd + 1,\n        })}\n    }\n\n    const onRmCart = (productId) => {\n        const availableBeforeRm = availableProducts[productId]\n        const inCartBeforeRm = inCartProducts[productId]\n        if (inCartBeforeRm>0) {\n        setAvailableProducts({\n            ...availableProducts,\n            [productId]: availableBeforeRm + 1,\n        })\n        \n        setInCartProducts({\n            ...inCartProducts,\n            [productId]: inCartBeforeRm - 1,\n        })}\n    }\n    // What is displayed. \n    // Display the Shop component and the Cart component. \n    return (\n        <div className=\"d-flex justify-content-between\">\n            <Shop products={products} onAddToCart={onAddToCart} availableProducts={availableProducts}/>\n            <Cart products={products} inCartProducts={inCartProducts} onRmCart={onRmCart}/>\n        </div>\n    )\n}\n\nReactDOM.render(<App />, document.getElementById('root'))\n"},"lineCount":null}},"error":null,"hash":"399879f0b3b7c0ff8e5261169f895f21","cacheData":{"env":{}}}